{
  "name": "ezio-api",
  "author": "ATJDEV Team",
  "description": "EZIO - API Server",
  "version": "2.59.1",
  "private": true,
  "license": "ATJ",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/atjdev/ezio-api.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "build:docker:test": "./scripts/build-test.sh",
    "build:docker:prod": "./scripts/build-prod.sh",
    "dev": "cross-env NODE_ENV=development ts-node-dev --poll --respawn --transpile-only src/index.ts",
    "start": "cross-env NODE_ENV=production pm2 start index.js --name \"ezio-api\" -i max",
    "start:alt": "cross-env NODE_ENV=production node index.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "db:migrate": "yarn typeorm migration:run",
    "db:revert": "yarn typeorm migration:revert",
    "db:seed:up": "yarn development src/db/seeds/seedsUp.ts",
    "db:seed:down": "yarn development src/db/seeds/seedsDown.ts",
    "g:entity": "yarn typeorm entity:create -n",
    "g:migration": "yarn typeorm migration:generate -n",
    "typeorm": "yarn development ./node_modules/typeorm/cli.js",
    "development": "DOTENV_CONFIG_PATH=./.env.development ts-node -P ./tsconfig.json -r dotenv/config",
    "gen-env": "gen-env-types .env.development -o src/types/env.d.ts -e . && rm -fr .env.example",
    "deploy": "./scripts/deploy.sh",
    "deploy:local": "./scripts/deploy-local.sh",
    "deploy:test": "./scripts/deploy-test.sh",
    "deploy:prod": "./scripts/deploy-prod.sh",
    "build:deploy:dev": "./scripts/build-deploy-dev.sh",
    "build:deploy:test": "./scripts/build-deploy-test.sh",
    "build:deploy:prod": "./scripts/build-deploy-prod.sh",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/request": "^2.48.8",
    "apollo-server-express": "^3.6.7",
    "argon2": "^0.28.5",
    "axios": "^0.26.1",
    "chalk": "4.1.2",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "connect-redis": "^6.1.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dataloader": "^2.0.0",
    "dayjs": "^1.11.3",
    "express": "^4.17.3",
    "express-session": "^1.17.1",
    "graphql": "15.8.0",
    "graphql-upload": "^13.0.0",
    "helmet": "^5.0.2",
    "ioredis": "^5.0.4",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "nodemailer": "^6.7.3",
    "oracledb": "^5.3.0",
    "pm2": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.5",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/axios": "^0.14.0",
    "@types/chance": "^1.1.0",
    "@types/compression": "^1.7.2",
    "@types/connect-redis": "^0.0.18",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.4",
    "@types/graphql-upload": "^8.0.11",
    "@types/ioredis": "^4.28.10",
    "@types/lodash": "^4.14.181",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.23",
    "@types/nodemailer": "^6.4.0",
    "@types/oracledb": "^5.2.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.18.0",
    "chance": "^1.1.8",
    "dotenv-flow": "^3.2.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gen-env-types": "^1.3.4",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0",
    "yarn": ">=1.22.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint"
    ]
  }
}
